<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="">
</head>

<body>
    <%- include('includes/navigation.ejs') %>
    <main>
        <% if (tasks.length > 0) { %>
            <div >
                <% for (let task of tasks) { %>
                    <article >
                        <header >
                            <h1 ><%= task.title %></h1>
                        </header>
                    </article>
                <% } %>
            </div>
        <% } else { %>
            <h1>No Tasks Found!</h1>
        <% } %>
    </main>
<%- include('includes/end.ejs') %>